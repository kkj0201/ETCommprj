
public class test {

	public static void main(String[] args) {
		final int size = 10;
		int[] seats = new int[size];
		
		while(true) {
			System.out.println("-------------------");
			for(int i = 0;i<size;i++)
			{
				System.out.println(i+1+"");			
			}
			System.out.println("\n-----------------------");
			for(int i = 0;i<size;i++)
			{
				System.out.println(seats[i]+"");	
			}
			System.out.println("\n-----------------------");
			
		}
	

	}
}
